          <FormulaText>Encounter.PartyMembers</FormulaText>
        </Collection>
        <MemberIndex>
          <FormulaText>partyMemberToAttack</FormulaText>
        </MemberIndex>
        <MemberIndexExpression>partyMemberToAttack</MemberIndexExpression>
      </RuleElementDef>
