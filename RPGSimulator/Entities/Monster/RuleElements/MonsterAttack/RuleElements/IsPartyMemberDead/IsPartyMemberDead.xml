      <ReturnType>Boolean</ReturnType>
      <FormulaText>IsClassification(Encounter.PartyMembers(partyMemberToAttack), Unconcious)</FormulaText>
    </Condition>
    <SubRules>
      <RuleElementDef
        xsi:type="FireNotificationActionDef"
        Revision="1"
        PublicRevision="1"
        Guid="acbfbda0-19b3-4fb0-866f-d184854b6426"
        Name="FireNotification1">
        <NotificationType>Informational</NotificationType>
        <NotificationMessage>
          <FormulaParseFormat>Token</FormulaParseFormat>
          <FormulaText>&lt;%Type &amp; " has killed " &amp; Encounter.PartyMembers(partyMemberToAttack).Name &amp; "!"%&gt;</FormulaText>
        </NotificationMessage>
      </RuleElementDef>
      <RuleElementDef
        xsi:type="RemoveCollectionMemberActionDef"
        Revision="1"
        PublicRevision="1"
        Guid="68b7b541-96d8-4bee-92d6-9a468ca78235"
        Name="RemoveCollectionMember2">
